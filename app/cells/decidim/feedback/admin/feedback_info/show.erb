<div class="reveal" id="<%= modal_id %>" data-reveal>
  <div class="reveal__header">
    <h2 class="reveal__title"><%= t(".title") %></h2>
    <button class="close-button" data-close aria-label="<%= t(".close_modal") %>"
      type="button">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="reveal__content" id="<%= modal_id %>-content" aria-live="assertive">
    <%== cell(
      "decidim/feedback/feedback_details",
      model,
      show_email: false,
      show_email_link: show_email_link?,
      show_contact: show_contact?
    ) %>
  </div>
</div>
